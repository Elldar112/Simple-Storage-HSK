<?xml version="1.0" encoding="utf-8" ?>
<Defs>
 
   	<!-- ==================== ResourcesCrate11 ==================== -->

  <ThingDef ParentName="JDStorageBase" >
    <defName>JDS_11ResourcesCrate</defName>
    <label>Supply Crate (Chunks)</label>
    <description>Can store up to 20 stacks of chunks</description>
    <graphicData>
      <texPath>Things/ResourcesCrate/ResourcesCrate11</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(1.3,1.3)</drawSize>
      <drawRotated>false</drawRotated>
    </graphicData>
    <uiIconPath>Things/ResourcesCrate/ResourcesCrate11</uiIconPath>
    <castEdgeShadows>true</castEdgeShadows>
    <size>(1,1)</size>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToBuild>600</WorkToBuild>
      <Flammability>1</Flammability>
      <Mass>15</Mass>
    </statBases>
    <costList>
	  <WoodPlank>70</WoodPlank>
      <ComponentMedieval>4</ComponentMedieval>
    </costList>
	<building>
      <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
      <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
	     <fixedStorageSettings>
        <priority>Critical</priority>
        <filter>
		<categories>
		<li>Chunks</li>
		</categories>
        </filter>
      </fixedStorageSettings>
    </building>		
    <comps>
      <li Class="LWM.DeepStorage.Properties" >
        <minNumberStacks>1</minNumberStacks>
        <maxNumberStacks>20</maxNumberStacks> <!-- In-game 10 -->
        <timeStoringTakes>120</timeStoringTakes>
        <showContents>false</showContents>
		<overlayType>SumOfAllItems</overlayType>
      </li>
    </comps>
		<researchPrerequisites>
	     <li>Storage_B4</li>
		</researchPrerequisites>
  </ThingDef>     

 
